package demoPackage;
public class mathOperators {
    Double a;
    Double b;
    public Double addTwoNumbers() { 
        Double sum = 0.0;
        sum = a + b;    
        return sum;
    }
    public Double subtractTwoNumbers() {    
        Double difference = 0.0;
        difference = a - b; 
        return difference;
    }
    public Double multiplyTwoNumbers() {    
        Double product = 0.0;
        product = a * b;    
        return product;
    }
}
